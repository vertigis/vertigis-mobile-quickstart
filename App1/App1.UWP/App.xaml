<?xml version="1.0" encoding="utf-8"?>
<maui:MauiWinUIApplication
    x:Class="App1.UWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:maui="using:Microsoft.Maui"
    xmlns:local="using:App1.UWP">
    <maui:MauiWinUIApplication.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="VertiGIS.Mobile/Platforms/Windows/VertiGISResources.xaml" />
                <!-- TODO: missing from nupkg? <ResourceDictionary Source="VertiGIS.Mobile.Toolkit/Resources/ToolkitResources.xaml" />-->
            </ResourceDictionary.MergedDictionaries>
            <Style x:Key="DefaultButtonStyle" TargetType="Button">
                <Setter Property="Background" Value="Blue"/>
            </Style>
        </ResourceDictionary>
    </maui:MauiWinUIApplication.Resources>
</maui:MauiWinUIApplication>
